<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Self_Reference</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISBLANK(Student__c)) &amp;&amp; 
Student__c = Id</errorConditionFormula>
    <errorMessage>A Guardian cannot reference themselves as the Student.</errorMessage>
</ValidationRule>
