<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Assignment_DueDate_NotBeforeCreated</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  NOT(ISBLANK(Due_Date__c)),
  Due_Date__c &lt; DATEVALUE(CreatedDate)
)</errorConditionFormula>
    <errorDisplayField>Due_Date__c</errorDisplayField>
    <errorMessage>Due Date cannot be earlier than the Assignment creation date.</errorMessage>
</ValidationRule>
